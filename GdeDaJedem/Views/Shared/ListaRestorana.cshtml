@model IEnumerable<GdeDaJedem.Domain.RestoranBO>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.restoranID)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Naziv)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Adresa)
        </th>
       
        
        
        <th>
            @Html.DisplayNameFor(model => model.NazivOpstine)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.restoranID)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.Naziv)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adresa)
        </td>
        
        
       
        <td>
            @Html.DisplayFor(modelItem => item.NazivOpstine)
        </td>
        
    </tr>
}

</table>
